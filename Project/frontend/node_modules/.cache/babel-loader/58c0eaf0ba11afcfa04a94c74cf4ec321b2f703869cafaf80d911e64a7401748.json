{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport headMenu from './headMenu.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    headMenu\n  },\n  data() {\n    return {\n      mainCategory: '',\n      items: [],\n      keyWord: ''\n    };\n  },\n  mounted() {\n    const getSameProsProduct = `http://192.168.0.213:3000/api/item/getSameProsProduct`;\n    axios.get(getSameProsProduct, {\n      params: {\n        \"itemId\": this.$store.state.itemId,\n        \"keyWord\": this.keyWord\n      }\n    }).then(response => {\n      this.items = response.data;\n      console.log(response.data);\n    }).catch(error => {\n      console.error('API 요청 중 오류 발생:', error);\n    });\n  },\n  created() {\n    this.keyWord = this.$route.query.keyWord;\n  },\n  methods: {\n    changeNum(value) {\n      if (value != null) {\n        let valueStr = value.toString();\n        valueStr = valueStr.split('.')[0];\n        return valueStr.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, \"$1,\");\n      } else {\n        return \"\";\n      }\n    },\n    clickItem(item) {\n      this.$store.dispatch('loadItemId', item);\n      this.$router.push({\n        name: \"itemDescription\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["headMenu","axios","components","data","mainCategory","items","keyWord","mounted","getSameProsProduct","get","params","$store","state","itemId","then","response","console","log","catch","error","created","$route","query","methods","changeNum","value","valueStr","toString","split","replace","clickItem","item","dispatch","$router","push","name"],"sources":["/Users/jinsanghyeok/Desktop/swe-2023/Project/frontend/src/components/similarItems.vue"],"sourcesContent":["<template>\n    <headMenu :menu=\"keyWord\" />\n    <div class=\"content container-fluid p-3\">\n        <div class=\"row\">\n            <div v-for=\"item in items\" :key=\"item.id\" class=\"col-6 mb-3\">\n                <div class=\"lists text-center\" @click=\"clickItem(item.ProductID)\">\n                    <img class=\"img-fluid\" :src=\"require(`@/assets/photos/items/${item.ProductID}.jpg`)\">\n                    <div class=\"description\">\n                        <div class=\"brand\">{{ item.Brand }}</div>\n                        <div class=\"name\">{{ item.ProductName }}</div>\n                        <div class=\"info\">\n                            <span class=\"discount\">{{ item.Discount }}%</span>\n                            <span class=\"discountedPrice\">{{ changeNum(item.Price * (1 - 0.01 * item.Discount)) }}원</span>\n                            <span class=\"price\">{{ changeNum(item.Price) }}원</span>\n                        </div>\n                        <div class=\"freeShipping text-start\">\n                            <div v-if=\"item.FreeShipping\" class=\"text-center\">무료배송</div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script scoped>\nimport headMenu from './headMenu.vue';\nimport axios from 'axios';\nexport default {\n    components: {\n        headMenu,\n    },\n    data() {\n        return {\n            mainCategory: '',\n            items: [],\n            keyWord: ''\n        };\n    },\n    mounted() {\n        const getSameProsProduct = `http://192.168.0.213:3000/api/item/getSameProsProduct`;\n        axios.get(getSameProsProduct, {\n            params: {\n                \"itemId\": this.$store.state.itemId,\n                \"keyWord\": this.keyWord,\n            }\n        })\n            .then((response) => {\n                this.items = response.data;\n                console.log(response.data);\n            })\n            .catch((error) => {\n                console.error('API 요청 중 오류 발생:', error);\n            });\n\n    },\n    created() {\n        this.keyWord = this.$route.query.keyWord;\n    },\n    methods: {\n        changeNum(value) {\n            if (value != null) {\n                let valueStr = value.toString();\n                valueStr = valueStr.split('.')[0];\n                return valueStr.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, \"$1,\");\n            } else {\n                return \"\";\n            }\n        },\n        clickItem(item) {\n            this.$store.dispatch('loadItemId', item);\n            this.$router.push({\n                name: \"itemDescription\",\n            });\n        },\n    },\n}\n</script>\n\n<style scoped>\n.content {\n    width: 100%;\n    overflow-x: hidden;\n    text-align: start;\n    margin-top: 40px;\n    height: 100%;\n}\n\n.freeShipping {\n    background-color: rgb(224, 224, 224);\n    width: 60px;\n    font-size: 14px;\n    border-radius: 20%;\n    color: gray;\n}\n\n.brand,\n.name,\n.info {\n    text-align: start;\n}\n\n.brand {\n    color: rgb(150, 150, 150);\n}\n\n.discount {\n    font-size: 18px;\n    color: rgb(125, 157, 214);\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.discountedPrice {\n    font-size: 18px;\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.price {\n    text-decoration: line-through;\n    color: rgb(150, 150, 150);\n    font-size: 12px;\n}\n</style>\n"],"mappings":";AA0BA,OAAOA,QAAO,MAAO,gBAAgB;AACrC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,MAAMC,kBAAiB,GAAK,uDAAsD;IAClFP,KAAK,CAACQ,GAAG,CAACD,kBAAkB,EAAE;MAC1BE,MAAM,EAAE;QACJ,QAAQ,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,MAAM;QAClC,SAAS,EAAE,IAAI,CAACP;MACpB;IACJ,CAAC,EACIQ,IAAI,CAAEC,QAAQ,IAAK;MAChB,IAAI,CAACV,KAAI,GAAIU,QAAQ,CAACZ,IAAI;MAC1Ba,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACZ,IAAI,CAAC;IAC9B,CAAC,EACAe,KAAK,CAAEC,KAAK,IAAK;MACdH,OAAO,CAACG,KAAK,CAAC,iBAAiB,EAAEA,KAAK,CAAC;IAC3C,CAAC,CAAC;EAEV,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACd,OAAM,GAAI,IAAI,CAACe,MAAM,CAACC,KAAK,CAAChB,OAAO;EAC5C,CAAC;EACDiB,OAAO,EAAE;IACLC,SAASA,CAACC,KAAK