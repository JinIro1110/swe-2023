{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport headMenu from './headMenu.vue';\nimport { skincares } from '@/assets/items/skincare';\nimport axios from 'axios';\nexport default {\n  components: {\n    headMenu\n  },\n  data() {\n    return {\n      mainCategory: '',\n      items: [],\n      subCateogory: ['a', 'b', 'c', 'd']\n    };\n  },\n  created() {\n    this.items = skincares;\n    this.mainCategory = this.$store.state.mainCategory;\n    console.log();\n    const Url = `http://localhost:3000/api/category/subCategoryNavBar/mainCategory=${this.mainCategory}`;\n    axios.get(Url).then(response => {\n      // API 응답에서 서브 카테고리 목록을 가져와 데이터에 할당\n      this.subcategories = response.data.subcategories;\n    }).catch(error => {\n      console.error('API 요청 중 오류 발생:', error);\n    });\n  },\n  methods: {\n    changeNum: function (value) {\n      if (typeof value !== 'string') {\n        value = value.toString();\n      }\n      return value.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, '$1,');\n    },\n    clickItem(item) {\n      this.$store.dispatch('updateItem', item);\n      this.$router.push({\n        name: \"itemDescription\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["headMenu","skincares","axios","components","data","mainCategory","items","subCateogory","created","$store","state","console","log","Url","get","then","response","subcategories","catch","error","methods","changeNum","value","toString","replace","clickItem","item","dispatch","$router","push","name"],"sources":["/Users/jinsanghyeok/Desktop/swe-2023/Project/frontend/src/components/itemsMenu.vue"],"sourcesContent":["<template>\n    <headMenu :menu=\"mainCategory\" />\n    <div class=\"content container-fluid p-3\">\n        <div class=\"row\">\n            <div v-for=\"item in items\" :key=\"item.id\" class=\"col-6 mb-3\">\n                <div class=\"lists text-center\" @click=\"clickItem(item.id)\">\n                    <img :src=\"require(`@/assets/photos/${item.image}`)\">\n                    <div class=\"description\">\n                        <div class=\"brand\">{{ item.brand }}</div>\n                        <div class=\"name\">{{ item.name }}</div>\n                        <div class=\"info\">\n                            <span class=\"discount\">{{ item.discount }}%</span>\n                            <span class=\"discountedPrice\">{{ changeNum(item.price * (1 - 0.01 * item.discount)) }}원</span>\n                            <span class=\"price\">{{ changeNum(item.price) }}원</span>\n                        </div>\n                        <div class=\"freeShipping text-start\">\n                            <div v-if=\"item.freeDelivery\" class=\"text-center\">무료배송</div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script scoped>\nimport headMenu from './headMenu.vue';\nimport { skincares } from '@/assets/items/skincare';\nimport axios from 'axios';\nexport default {\n    components: {\n        headMenu,\n    },\n    data() {\n        return {\n            mainCategory: '',\n            items: [],\n            subCateogory: ['a', 'b', 'c', 'd']\n        };\n    },\n    created() {\n        this.items = skincares;\n        this.mainCategory = this.$store.state.mainCategory;\n        console.log()\n\n        const Url = `http://localhost:3000/api/category/subCategoryNavBar/mainCategory=${this.mainCategory}`;\n\n        axios.get(Url)\n            .then((response) => {\n                // API 응답에서 서브 카테고리 목록을 가져와 데이터에 할당\n                this.subcategories = response.data.subcategories;\n            })\n            .catch((error) => {\n                console.error('API 요청 중 오류 발생:', error);\n            });\n    },\n\n    methods: {\n        changeNum: function (value) {\n            if (typeof value !== 'string') {\n                value = value.toString();\n            }\n            return value.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, '$1,');\n        },\n        clickItem(item) {\n            this.$store.dispatch('updateItem', item);\n            this.$router.push({\n                name: \"itemDescription\",\n            });\n        }\n    },\n}\n</script>\n\n<style scoped>\n.content {\n    width: 100%;\n    overflow-x: hidden;\n    text-align: start;\n    margin-top: 40px;\n    height: 100%;\n}\n\n.freeShipping {\n    background-color: rgb(224, 224, 224);\n    width: 60px;\n    font-size: 14px;\n    border-radius: 20%;\n    color: gray;\n}\n\n.brand,\n.name,\n.info {\n    text-align: start;\n}\n\n.brand {\n    color: rgb(150, 150, 150);\n}\n\n.discount {\n    font-size: 18px;\n    color: rgb(125, 157, 214);\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.discountedPrice {\n    font-size: 18px;\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.price {\n    text-decoration: line-through;\n    color: rgb(150, 150, 150);\n    font-size: 12px;\n}</style>\n"],"mappings":";AA0BA,OAAOA,QAAO,MAAO,gBAAgB;AACrC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,CAAC,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG;IACrC,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACF,KAAI,GAAIL,SAAS;IACtB,IAAI,CAACI,YAAW,GAAI,IAAI,CAACI,MAAM,CAACC,KAAK,CAACL,YAAY;IAClDM,OAAO,CAACC,GAAG,CAAC;IAEZ,MAAMC,GAAE,GAAK,qEAAoE,IAAI,CAACR,YAAa,EAAC;IAEpGH,KAAK,CAACY,GAAG,CAACD,GAAG,EACRE,IAAI,CAAEC,QAAQ,IAAK;MAChB;MACA,IAAI,CAACC,aAAY,GAAID,QAAQ,CAACZ,IAAI,CAACa,aAAa;IACpD,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;MACdR,OAAO,CAACQ,KAAK,CAAC,iBAAiB,EAAEA,KAAK,CAAC;IAC3C,CAAC,CAAC;EACV,CAAC;EAEDC,OAAO,EAAE;IACLC,SAAS,EAAE,SAAAA,CAAUC,KAAK,EAAE;MACxB,IAAI,OAAOA,KAAI,KAAM,QAAQ,EAAE;QAC3BA,KAAI,GAAIA,KAAK,CAACC,QAAQ,CAAC,CAAC;MAC5B;MACA,OAAOD,KAAK,CAACE,OAAO,CAAC,2BAA2B,EAAE,KAAK,CAAC;IAC5D,CAAC;IACDC,SAASA,CAACC,IAAI,EAAE;MACZ,IAAI,CAACjB,MAAM,CAACkB,QAAQ,CAAC,YAAY,EAAED,IAAI,CAAC;MACxC,IAAI,CAA