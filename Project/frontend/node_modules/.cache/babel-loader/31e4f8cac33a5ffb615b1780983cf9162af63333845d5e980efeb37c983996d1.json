{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport headMenu from './headMenu.vue';\nimport { skincares } from '@/assets/items/skincare';\nimport axios from 'axios';\nexport default {\n  components: {\n    headMenu\n  },\n  data() {\n    return {\n      mainCategory: '',\n      subCategories: [],\n      items: [],\n      activeTab: 0\n    };\n  },\n  created() {\n    this.items = skincares;\n    this.mainCategory = this.$store.state.mainCategory;\n    const getSubCategoryTab = `http://localhost:3000/api/category/subCategoryNavBar/${this.mainCategory}`;\n    axios.get(getSubCategoryTab).then(response => {\n      this.subCategories = response.data.subcategories;\n    }).catch(error => {\n      console.error('API 요청 중 오류 발생:', error);\n    });\n    const getMainCategoryItems = `http://localhost:3000/api/category/showEntireItem/${this.mainCategory}`;\n    axios.get(getMainCategoryItems).then(response => {\n      this.items = response.data.mainCategoryItemData;\n      console.log(response.data.mainCategoryItemData[0]);\n      console.log(this.items);\n    }).catch(error => {\n      console.error('API 요청 중 오류 발생:', error);\n    });\n  },\n  methods: {\n    changeNum: function (value) {\n      if (typeof value !== 'string') {\n        value = value.toString();\n      }\n      return value.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, '$1,');\n    },\n    clickItem(item) {\n      this.$store.dispatch('updateItem', item);\n      this.$router.push({\n        name: \"itemDescription\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["headMenu","skincares","axios","components","data","mainCategory","subCategories","items","activeTab","created","$store","state","getSubCategoryTab","get","then","response","subcategories","catch","error","console","getMainCategoryItems","mainCategoryItemData","log","methods","changeNum","value","toString","replace","clickItem","item","dispatch","$router","push","name"],"sources":["/Users/jinsanghyeok/Desktop/swe-2023/Project/frontend/src/components/itemsMenu.vue"],"sourcesContent":["<template>\n    <headMenu :menu=\"mainCategory\" />\n    <div class=\"subCategoryTab container-fluid align-items-center\">\n        <div class=\"row\">\n            <div class=\"col\">\n                <div class=\"bar\">\n                    <ul class=\"nav nav-pills nav-fill\">\n                        <li class=\"nav-item\">\n                            <div class=\"nav-link\" :class=\"{ active: activeTab === 0 }\"\n                                @click=\"activeTab = 0\">전체</div>\n                        </li>\n                        <li v-for=\"(subCategory, index) in subCategories\" :key=\"index\" class=\"nav-item\">\n                            <div class=\"nav-link\" :class=\"{ active: activeTab === index + 1 }\"\n                                @click=\"activeTab = index + 1\">{{\n                                    subCategory.SubCategoryName }}</div>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n\n    </div>\n    <div class=\"content container-fluid p-3\">\n        <div class=\"row\">\n            <div v-for=\"item in items\" :key=\"item.id\" class=\"col-6 mb-3\">\n                <div class=\"lists text-center\" @click=\"clickItem(item.id)\">\n                    <img :src=\"require(`@/assets/photos/${item.image}`)\">\n                    <div class=\"description\">\n                        <div class=\"brand\">{{ item.brand }}</div>\n                        <div class=\"name\">{{ item.name }}</div>\n                        <div class=\"info\">\n                            <span class=\"discount\">{{ item.discount }}%</span>\n                            <span class=\"discountedPrice\">{{ changeNum(item.price * (1 - 0.01 * item.discount)) }}원</span>\n                            <span class=\"price\">{{ changeNum(item.price) }}원</span>\n                        </div>\n                        <div class=\"freeShipping text-start\">\n                            <div v-if=\"item.freeDelivery\" class=\"text-center\">무료배송</div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script scoped>\nimport headMenu from './headMenu.vue';\nimport { skincares } from '@/assets/items/skincare';\nimport axios from 'axios';\nexport default {\n    components: {\n        headMenu,\n    },\n    data() {\n        return {\n            mainCategory: '',\n            subCategories: [],\n            items: [],\n            activeTab: 0,\n        };\n    },\n    created() {\n        this.items = skincares;\n        this.mainCategory = this.$store.state.mainCategory;\n\n        const getSubCategoryTab = `http://localhost:3000/api/category/subCategoryNavBar/${this.mainCategory}`;\n\n        axios.get(getSubCategoryTab)\n            .then((response) => {\n                this.subCategories = response.data.subcategories;\n            })\n            .catch((error) => {\n                console.error('API 요청 중 오류 발생:', error);\n            });\n        \n        const getMainCategoryItems = `http://localhost:3000/api/category/showEntireItem/${this.mainCategory}`;\n            axios.get(getMainCategoryItems)\n            .then((response) => {\n                this.items = response.data.mainCategoryItemData;\n                console.log(response.data.mainCategoryItemData[0]);\n                console.log(this.items);\n            })\n            .catch((error) => {\n                console.error('API 요청 중 오류 발생:', error);\n            })\n    },\n\n    methods: {\n        changeNum: function (value) {\n            if (typeof value !== 'string') {\n                value = value.toString();\n            }\n            return value.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, '$1,');\n        },\n        clickItem(item) {\n            this.$store.dispatch('updateItem', item);\n            this.$router.push({\n                name: \"itemDescription\",\n            });\n        }\n    },\n}\n</script>\n\n<style scoped>\n.content {\n    width: 100%;\n    overflow-x: hidden;\n    text-align: start;\n    margin-top: 40px;\n    height: 100%;\n}\n\n.freeShipping {\n    background-color: rgb(224, 224, 224);\n    width: 60px;\n    font-size: 14px;\n    border-radius: 20%;\n    color: gray;\n}\n\n.brand,\n.name,\n.info {\n    text-align: start;\n}\n\n.brand {\n    color: rgb(150, 150, 150);\n}\n\n.discount {\n    font-size: 18px;\n    color: rgb(125, 157, 214);\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.discountedPrice {\n    font-size: 18px;\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.price {\n    text-decoration: line-through;\n    color: rgb(150, 150, 150);\n    font-size: 12px;\n}\n\n.nav-link {\n    color: gray;\n    border-bottom: 3px solid transparent;\n    border-radius: 0;\n}\n\n.nav-link:hover {\n    color: darkgray;\n}\n\n.nav-link.active {\n    color: #5BF52F;\n    border-bottom: 3px solid #5BF52F;\n    background-color: transparent;\n}\n\n.subCategoryTab {\n    margin-top: 50px;\n}\n</style>\n"],"mappings":";AA8CA,OAAOA,QAAO,MAAO,gBAAgB;AACrC,SAASC,SAAQ,QAAS,yBAAyB;AACnD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE,EAAE;MACjBC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACF,KAAI,GAAIN,SAAS;IACtB,IAAI,CAACI,YAAW,GAAI,IAAI,CAACK,MAAM,CAACC,KAAK,CAACN,YAAY;IAElD,MAAMO,iBAAgB,GAAK,wDAAuD,IAAI,CAACP,YAAa,EAAC;IAErGH,KAAK,CAACW,GAAG,CAACD,iBAAiB,EACtBE,IAAI,CAAEC,QAAQ,IAAK;MAChB,IAAI,CAACT,aAAY,GAAIS,QAAQ,CAACX,IAAI,CAACY,aAAa;IACpD,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;MACdC,OAAO,CAACD,KAAK,CAAC,iBAAiB,EAAEA,KAAK,CAAC;IAC3C,CAAC,CAAC;IAEN,MAAME,oBAAmB,GAAK,qDAAoD,IAAI,CAACf,YAAa,EAAC;IACjGH,KAAK,CAACW,GAAG,CAACO,oBAAoB,EAC7BN,IAAI,CAAEC,QAAQ,IAAK;MAChB,IAAI,CAACR,KAAI,GAAIQ,QAAQ,CAACX,IAAI,CAACiB,oBAAoB;MAC/CF,OAAO,CAACG,GAAG,CAACP,QAAQ,CAACX,IAAI