{"ast":null,"code":"import headMenu from './headMenu.vue';\nimport { skincares } from '@/assets/items/skincare';\nexport default {\n  mounted() {\n    this.menu = this.$store.state.menu;\n  },\n  components: {\n    headMenu\n  },\n  data() {\n    return {\n      menu: '',\n      items: []\n    };\n  },\n  created() {\n    this.items = skincares;\n  },\n  methods: {\n    changeNum: function (value) {\n      if (typeof value !== 'string') {\n        value = value.toString();\n      }\n      return value.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, '$1,');\n    }\n  }\n};","map":{"version":3,"names":["headMenu","skincares","mounted","menu","$store","state","components","data","items","created","methods","changeNum","value","toString","replace"],"sources":["/Users/jinsanghyeok/Desktop/front/src/components/itemsMenu.vue"],"sourcesContent":["<template>\n    <headMenu :menu=\"menu\" />\n    <div class=\"content container-fluid\">\n        <div class=\"row\">\n            <div v-for=\"item in items\" :key=\"item.id\" class=\"col\">\n                <div class=\"lists\">\n                    <img :src=\"require(`@/assets/photos/${item.image}`)\" class=\"img-fluid\">\n                    <div class=\"description\">\n                        <div class=\"brand\">{{ item.brand }}</div>\n                        <div class=\"name\">{{ item.name }}</div>\n                        <span class=\"discount\">{{ item.discount }}%</span>\n                        <span class=\"price\">{{ changeNum(item.price) }}원</span>\n                        <span class=\"discountedPrice\">{{ changeNum(item.price * (1 - 0.01 * item.discount)) }}원</span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script scoped>\nimport headMenu from './headMenu.vue'\nimport { skincares } from '@/assets/items/skincare'\nexport default {\n    mounted() {\n        this.menu = this.$store.state.menu;\n    },\n    components: {\n        headMenu,\n    },\n    data() {\n        return {\n            menu: '',\n            items: [],\n        };\n    },\n    created() {\n        this.items = skincares;\n    },\n    methods : {\n        changeNum: function(value) {\n            if (typeof value !== 'string') {\n                value = value.toString();\n            }\n            return value.replace(/(\\d)(?=(?:\\d{3})+(?!\\d))/g, '$1,');\n        },\n    }\n}\n</script>\n\n<style>\n.content {\n    margin: 70px 0;\n}\n\n.brand, .name {\n    text-align: start;\n}\n.discount {\n    font-size: 20px;\n    color: rgb(125, 157, 214);\n    font-weight: bold;\n    margin-right: 3px;\n}\n\n.price {\n    font-size: 20px;\n    font-weight: bold;\n}\n.discountedPrice {\n    text-decoration: line-through;\n    color: rgb(150, 150, 150);\n}\n</style>\n"],"mappings":"AAqBA,OAAOA,QAAO,MAAO,gBAAe;AACpC,SAASC,SAAQ,QAAS,yBAAwB;AAClD,eAAe;EACXC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,IAAG,GAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,IAAI;EACtC,CAAC;EACDG,UAAU,EAAE;IACRN;EACJ,CAAC;EACDO,IAAIA,CAAA,EAAG;IACH,OAAO;MACHJ,IAAI,EAAE,EAAE;MACRK,KAAK,EAAE;IACX,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACD,KAAI,GAAIP,SAAS;EAC1B,CAAC;EACDS,OAAM,EAAI;IACNC,SAAS,EAAE,SAAAA,CAASC,KAAK,EAAE;MACvB,IAAI,OAAOA,KAAI,KAAM,QAAQ,EAAE;QAC3BA,KAAI,GAAIA,KAAK,CAACC,QAAQ,CAAC,CAAC;MAC5B;MACA,OAAOD,KAAK,CAACE,OAAO,CAAC,2BAA2B,EAAE,KAAK,CAAC;IAC5D;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}